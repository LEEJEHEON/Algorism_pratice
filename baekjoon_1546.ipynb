    "n = int(input())\n",
    "new_average = 0\n",
    "score_list = []\n",
    "\n",
    "score_list.extend(input().split())\n",
    "score_list= list(map(int,score_list))\n",
    "\n",
    "for j in score_list:\n",
    "    new_average += (j/max(score_list)*100)\n",
    "    \n",
    "print(new_average / n)"
   ]
  }
